{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": ["dummy"],
  "extends": [
    "config:base",
    "group:allNonMajor"
  ],
  "docker": {
    "enabled": true,
    "pinDigests": false,
    "major": {
      "enabled": true
    },
    "minor": {
      "enabled": true
    },
    "patch": {
      "enabled": false
    }
  },
  "schedule": [
    "every weekday"
  ]
}
